{
  "manifest_version": 2,
  "name": "Single User Dash",
  "description": "Single User Dash",
  "version": "1.0",
  "background":{
	"scripts":["singleuserdash.background.js"]
  },
  "page_action": {
    "default_icon":{
		"38":"icon38.png",
		"19":"icon19.png",
		"16":"icon16.png"
	  },
	"default_title": "Single User Dash"
  }, 
  "permissions":["windows","tabs","http://*.tumblr.com/*","https://*.tumblr.com/*"],
  "content_scripts": [{
	"js": ["singleuserdash.contentscript.js"],
	"run_at": "document_start",
	"matches": ["http://*.tumblr.com/*", "https://*.tumblr.com/*"],
	"all_frames":false
  }],
  "author": "Alex Pendleton",
  "omnibox": { "keyword":"sud" }
}